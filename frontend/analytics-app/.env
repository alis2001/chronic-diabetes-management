# frontend/analytics-app/.env
# Environment configuration for Analytics Frontend Application

# React App Configuration
REACT_APP_NAME=Analytics Laboratorio ASL
REACT_APP_VERSION=1.0.0

# API Gateway Configuration (CRITICAL - All API calls go through Gateway)
REACT_APP_API_GATEWAY_URL=http://localhost:8080

# Service URLs (Fallback for development/debugging - not used in production)
REACT_APP_ANALYTICS_API_URL=http://localhost:8002
REACT_APP_TIMELINE_API_URL=http://localhost:8001

# Development Configuration
CHOKIDAR_USEPOLLING=true
FAST_REFRESH=true
ESLINT_NO_DEV_ERRORS=true
DISABLE_ESLINT_PLUGIN=false

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false

# Browser Configuration
BROWSER=none
SKIP_PREFLIGHT_CHECK=true

# Analytics Configuration
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_LOGGING=true

# Medical Data Configuration
REACT_APP_MAX_CHART_POINTS=1000
REACT_APP_CHART_ANIMATION_DURATION=750
REACT_APP_DEFAULT_CHART_COLOR=#10b981

# Iframe Configuration
REACT_APP_IFRAME_MODE=true
REACT_APP_PARENT_ORIGIN=http://localhost:3010

# Error Handling
REACT_APP_SHOW_ERROR_DETAILS=true
REACT_APP_ERROR_REPORTING_ENABLED=false